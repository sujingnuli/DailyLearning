@model IEnumerable<Album>

@{ViewBag.Title = "Lists Albums";}
<script src="~/Scripts/jquery-1.8.2.js"></script>
<ul>
    @foreach (var p in Model) { 
        <li>@p.Title</li>
    }
</ul>
@{
    var time = DateTime.Now;
    }
<span>@time</span>
<div id="result"></div>
<ol id="ull">
    <li><a>1</a></li>
    <li><a>2</a></li>
    <li><a>3</a></li>
    <li><a>4</a></li>
    <li><a>5</a></li>
</ol>
<script type="text/javascript">
    $(function () {
        $("#ull a").click(function () {
            var val = $(this).text();
            $("#result").load("/Album/AlbumTotal?total=" + val);
        });
    
    });
</script>
